<?xml version="1.0"?>
<project name="key.core.test" default="generateproofcollectionparser" basedir=".">
	<target name="generateproofcollectionparser">
		<!-- Call original ant file but with changed build.dir -->
		<ant antfile="${basedir}/../../../../key/key.core.test/build.xml" target="generateproofcollectionparser" dir="${basedir}/../../../../key/key.core.test">
			<property name="build.dir" value="${basedir}/bin" />
		</ant>
	</target>
	
	<target name="clean">
		<!-- Call original ant file but with changed build.dir -->
		<ant antfile="${basedir}/../../../../key/key.core.test/build.xml" target="clean" dir="${basedir}/../../../../key/key.core.test">
			<property name="build.dir" value="${basedir}/bin" />
		</ant>
	</target>
</project>